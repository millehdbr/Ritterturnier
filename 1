{
    class Program
    {
        static void Main(string[] args)
        {
            //string knight = "knight";


            Console.WriteLine("Wie viele Ritter sollen k√§mpfen?");
string knight = Console.ReadLine();

            switch (knight)
            {
                case "knight 1":
                    Console.WriteLine("White");
                    break;

                case "knight 2":
                    Console.WriteLine("Black");
                    break;

                case "knight 3":
                    Console.WriteLine("Blue");
                    break;
            }
        }

    }
